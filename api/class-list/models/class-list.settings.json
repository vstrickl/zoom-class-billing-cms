{
  "kind": "collectionType",
  "collectionName": "class_lists",
  "info": {
    "name": "Class List",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "classname": {
      "type": "string",
      "required": true
    },
    "zoomurl": {
      "type": "string",
      "required": true
    },
    "days": {
      "type": "string",
      "required": true
    },
    "start": {
      "type": "time"
    },
    "end": {
      "type": "time"
    },
    "new_students": {
      "via": "class_list",
      "collection": "new-student"
    },
    "class_img": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
